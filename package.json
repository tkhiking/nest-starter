{
  "name": "nestjs-starter",
  "version": "0.1.0",
  "description": "NestJS starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts'",
    "format": "eslint --fix '{src,apps,libs,test}/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -f src/config/ormconfig.postgres.ts -n",
    "migration:show": "ts-node ./node_modules/typeorm/cli.js migration:show -f src/config/ormconfig.postgres.ts",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -f src/config/ormconfig.postgres.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -f src/config/ormconfig.postgres.ts",
    "schema:log": "ts-node ./node_modules/typeorm/cli.js schema:log -f src/config/ormconfig.postgres.ts",
    "doc": "compodoc -p tsconfig.json -s"
  },
  "dependencies": {
    "@godaddy/terminus": "4.3.1",
    "@nestjs/common": "7.0.6",
    "@nestjs/core": "7.0.6",
    "@nestjs/graphql": "^7.1.3",
    "@nestjs/jwt": "7.0.0",
    "@nestjs/microservices": "7.0.6",
    "@nestjs/passport": "7.0.0",
    "@nestjs/platform-express": "7.0.6",
    "@nestjs/swagger": "4.5.1",
    "@nestjs/terminus": "7.0.0-pre.4",
    "@nestjs/typeorm": "7.0.0",
    "@nestjs/websockets": "7.0.6",
    "apollo-server-express": "2.11.0",
    "bcrypt": "4.0.1",
    "class-transformer": "0.2.3",
    "class-validator": "0.11.0",
    "compression": "1.7.4",
    "graphql": "14.6.0",
    "graphql-tools": "4.0.7",
    "helmet": "3.22.0",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-mock-strategy": "2.0.0",
    "pg": "7.18.2",
    "pluralize": "8.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.4",
    "sqlite3": "4.1.1",
    "swagger-ui-express": "4.1.4",
    "type-graphql": "0.17.6",
    "typeorm": "0.2.24"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.11",
    "@nestjs/cli": "7.1.1",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.6",
    "@types/bcrypt": "3.0.0",
    "@types/express": "4.17.3",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.5",
    "@types/passport-jwt": "3.0.3",
    "@types/pluralize": "0.0.29",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "jest": "25.2.3",
    "prettier": "2.0.2",
    "supertest": "4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.2",
    "ts-node": "8.8.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
